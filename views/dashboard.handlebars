<nav class="navbar navbar-inverse navbar-fixed-top">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="/dashboard">EncAssess</a>
    </div>
    <div id="navbar" class="navbar-collapse collapse">
      <ul class="nav navbar-nav navbar-right">
        <li><a id="logout-link" href="/logout">Logout</a></li>
      </ul>
      <form class="navbar-form navbar-right">
        <input type="text" class="form-control" placeholder="Search...">
      </form>
    </div>
  </div>
</nav>

<div class="container-fluid">
  <div class="row">
    <div class="col-sm-3 col-md-2 sidebar">
      <ul class="nav nav-sidebar">
        <li class="active"><a href="#">Menu 1 <span class="sr-only">(current)</span></a></li>
        <li><a href="#">Menu 2</a></li>
        <li><a href="#">Menu 3</a></li>
      </ul>
    </div>
    <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
      <h1 class="page-header">Welcome {{username}}!</h1>




      <div class="table-responsive">
        <table class="table table-striped">
          <thead>
            <tr>
              <th>Time</th>
              <th>Patient</th>
              <th>Care</th>
              <th>Notes</th>
            </tr>
          </thead>
          <tbody>
            <form method="POST" action="/dashboard" id="createDay">
              {{#each times}}
              <tr id="entry-table">
                <td>{{this}}</td>
                <td><input type="text" class="entry" name="entry" autofocus></td>
                <td><input type="text" class="entry" name="entry" autofocus></td>
                <td><input type="text" class="entry" name="entry" autofocus></td>
              </tr>
              {{/each}}
              <h2 class="sub-header" id="currentDate" name="currentDate">{{now}}</h2>
              <button class="btn btn-lg btn-primary btn-block" type="submit" id="entry-submit">Submit</button>
            </form>
          </tbody>
        </table>

      </div>
    </div>
  </div>
</div>

